<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.apache.streampark.console.plugin.mapper.ApplicationOfJobMapper">
    <resultMap id="BaseResultMap" type="org.apache.streampark.console.plugin.entity.ApplicationOfJob">
        <id column="id" jdbcType="BIGINT" property="id"/>
        <result column="app_id" jdbcType="BIGINT" property="appId"/>
        <result column="job_id" jdbcType="VARCHAR" property="jobId"/>
        <result column="create_time" jdbcType="DATE" property="createTime"/>
    </resultMap>

    <select id="getByJobId" resultType="org.apache.streampark.console.plugin.entity.ApplicationOfJob" parameterType="java.lang.String">
        select *
        from t_application_of_job
        where job_id=#{jobId}
    </select>

</mapper>
